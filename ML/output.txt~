%%%-----------------------------------------------------------------Test 2
- piglatinize "sandwich";
val it = "andwichsay" : string

%%%-----------------------------------------------------------------Test 3
- piglatinize "apple";
val it = "appleyay" : string

%%%--------------------------------------Piglatinize Helper Function Tests
%%%-----------------------------------------------------------member Tests
%%%-----------------------------------------------------------------Test 1
- member(1, [1,2,3,4]);
val it = true : bool

%%%-----------------------------------------------------------------Test 2
- member(5, [1,2,3,4]);
val it = false : bool

%%%-----------------------------------------------------------------Test 3
- member(#"a", [#"a", #"b"]);
val it = true : bool

%%%-----------------------------------------------------------------Test 4
- member(#"z", [#"a", #"b"]);
val it = false : bool

%%%-----------------------------------------------------------------Test 5
- member("abc", ["abc", "def"]);
val it = true : bool

%%%-----------------------------------------------------------------Test 6
- member("ghi", ["abc", "def"]);
val it = false : bool

%%%----------------------------------------------------------isVowel Tests
%%%-----------------------------------------------------------------Test 1
- isVowel(#"a");
