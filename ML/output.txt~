%%%-----------------------------------------------------------------Test 3
- piglatinizer([#"a", #"b", #"l", #"e"]);
val it = [#"a",#"b",#"l",#"e"] : char list
%%%--------------------------------------------piglatinizeController Tests
%%%-----------------------------------------------------------------Test 1
