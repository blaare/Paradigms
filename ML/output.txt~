val it = false : bool

%%%-----------------------------------------------------piglatinizer Tests
%%%-----------------------------------------------------------------Test 1
-  piglatinizer([#"s",#"t",#"r",#"i",#"p",#"e"]);
val it = [#"i",#"p",#"e",#"s",#"t",#"r"] : char list

%%%-----------------------------------------------------------------Test 2
-  piglatinizer([#"t",#"e",#"s",#"t",#"i",#"n", #"g"]);
